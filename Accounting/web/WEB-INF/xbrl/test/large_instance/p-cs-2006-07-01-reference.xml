    <link:reference xlink:type="resource" xlink:label="reference_CreditRiskMitigationTechniquesAffectingAmountExposureFundedCreditProtectionFinancialCollateralComprenhensiveMethod" xlink:role="http://www.xbrl.org/2003/role/reference" xlink:title="reference_CreditRiskMitigationTechniquesAffectingAmountExposureFundedCreditProtectionFinancialCollateralComprenhensiveMethod" id="reference_CreditRiskMitigationTechniquesAffectingAmountExposureFundedCreditProtectionFinancialCollateralComprenhensiveMethod">
      <ref-corep:Comment>Annex VIII part 3 points 30 to 59 of Directive 2006/48/EC. It also includes credit linked notes (Annex VIII part 3 point 3 of Directive 2006/48/EC)</ref-corep:Comment>
    </link:reference>
    <link:referenceArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-reference" xlink:from="CreditRiskMitigationTechniquesAffectingAmountExposureFundedCreditProtectionFinancialCollateralComprenhensiveMethod" xlink:to="reference_CreditRiskMitigationTechniquesAffectingAmountExposureFundedCreditProtectionFinancialCollateralComprenhensiveMethod" xlink:title="reference: CreditRiskMitigationTechniquesAffectingAmountExposureFundedCreditProtectionFinancialCollateralComprenhensiveMethod to reference_CreditRiskMitigationTechniquesAffectingAmountExposureFundedCreditProtectionFinancialCollateralComprenhensiveMethod"/>
    <link:loc xlink:type="locator" xlink:href="p-cs-2006-07-01.xsd#p-cs_VolatilityAdjustmentExposure" xlink:label="VolatilityAdjustmentExposure" xlink:title="VolatilityAdjustmentExposure"/>
    <link:reference xlink:type="resource" xlink:label="reference_VolatilityAdjustmentExposure" xlink:role="http://www.xbrl.org/2003/role/reference" xlink:title="reference_VolatilityAdjustmentExposure" id="reference_VolatilityAdjustmentExposure">
      <ref-corep:Comment>Annex VIII part 3 point 33 of Directive 2006/48/EC. The amount to be reported is given by the impact of  the volatility adjustment to the exposure (Eva-E) = E*He</ref-corep:Comment>
    </link:reference>
    <link:referenceArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-reference" xlink:from="VolatilityAdjustmentExposure" xlink:to="reference_VolatilityAdjustmentExposure" xlink:title="reference: VolatilityAdjustmentExposure to reference_VolatilityAdjustmentExposure"/>
    <link:loc xlink:type="locator" xlink:href="p-cs-2006-07-01.xsd#p-cs_FinancialCollateralAdjustedValueCvam" xlink:label="FinancialCollateralAdjustedValue" xlink:title="FinancialCollateralAdjustedValue"/>
