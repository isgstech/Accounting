    <link:referenceArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-reference" xlink:from="FullyAdjustedExposureValue" xlink:to="reference_FullyAdjustedExposureValue" xlink:title="reference: FullyAdjustedExposureValue to reference_FullyAdjustedExposureValue"/>
